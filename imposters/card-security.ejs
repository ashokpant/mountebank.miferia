{
        "protocol": "grpc",
        "port": 2571,
        "loglevel": "debug",
        "recordRequests": true,
        "services": {
            "miferia.rpc.security.SecurityRpc": {
                "file": "src/security/security_rpc.proto"
            }
        },
        "options": {
            "protobufjs": {
                "includeDirs": [
                    "/app/proto.miferia",
                    "/app/proto.miferia/src"
                ]
            }
        },
        "stubs": [
            {
                "predicates": [
                    {
                        "matches": {
                            "path": "encrypt"
                        },
                        "caseSensitive": false
                    }
                ],
                "responses": [
                    {
                        "is": {
                            "value": {
                                "error": false,
                                "success": true,
                                "encryptResponse": {
                                    "cipherBytes": "",
                                    "cipherStr": ""
                                }
                            }
                        }
                    }
                ]
            },
            {
                "predicates": [
                    {
                        "matches": {
                            "path": "decrypt"
                        },
                        "caseSensitive": false
                    }
                ],
                "responses": [
                    {
                        "is": {
                            "value": {
                                "error": false,
                                "success": true,
                                "decryptResponse": {
                                    "message": "MTIzNDU2Nzg5MDEyMzQ1Ng=="
                                }
                            }
                        }
                    }
                ]
            }
        ],
        "callbackURLTemplate": "http://localhost:2525/imposters/:port/_requests"
}